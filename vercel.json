{
  "version": 2,
  "name": "uwp-dev-q7a",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/unity-builds/.*\\.loader\\.js$",
      "dest": "$0",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/unity-builds/.*\\.framework\\.js\\.gz$",
      "dest": "$0",
      "headers": {
        "Content-Encoding": "gzip",
        "Content-Type": "text/javascript",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/unity-builds/.*\\.data\\.gz$",
      "dest": "$0",
      "headers": {
        "Content-Encoding": "gzip",
        "Content-Type": "application/octet-stream",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/unity-builds/.*\\.wasm\\.gz$",
      "dest": "$0",
      "headers": {
        "Content-Encoding": "gzip",
        "Content-Type": "application/octet-stream",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/unity-builds/(.*)",
      "dest": "/unity-builds/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|mp4|webp))",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "functions": {
    "app/[[...route]].js": {
      "maxDuration": 30
    }
  }
}